//CHECK-NEXT:      function.return %[[SELF]] : !struct.type<@CJ>
//CHECK-NEXT:    }
//CHECK-NEXT:    function.def @constrain(%[[SELF:[0-9a-zA-Z_\.]+]]: !struct.type<@CJ>) attributes {function.allow_constraint} {
