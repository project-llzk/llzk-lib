// CHECK:     %arg1 in TypeA:[ 0, 1 ]
// CHECK: }
// CHECK-LABEL: @CheckBitInv StructIntervals {
// CHECK:     %arg1 in TypeA:[ 0, 1 ]
// CHECK: }

