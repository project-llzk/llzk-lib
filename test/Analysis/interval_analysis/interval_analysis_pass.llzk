module attributes {llzk.lang} {
  struct.def @MemberRangeConstraint {

    struct.member @foo : !felt.type
    struct.member @bar : !felt.type

    function.def @compute(%a: !felt.type, %b: !felt.type) -> !struct.type<@MemberRangeConstraint> {
      %self = struct.new : !struct.type<@MemberRangeConstraint>
      function.return %self : !struct.type<@MemberRangeConstraint>
    }

    function.def @constrain(%self: !struct.type<@MemberRangeConstraint>, %a: !felt.type, %b: !felt.type) {
      %x = felt.const 1
      %y = felt.const 2
      %cmpA = bool.cmp gt(%a, %x)
      bool.assert %cmpA
      %cmpB = bool.cmp le(%b, %y)
      bool.assert %cmpB
      %foo = struct.readm %self[@foo] : !struct.type<@MemberRangeConstraint>, !felt.type
      constrain.eq %foo, %x : !felt.type
      %bar = struct.readm %self[@bar] : !struct.type<@MemberRangeConstraint>, !felt.type
      constrain.eq %bar, %b : !felt.type
      function.return
    }
  }
}

